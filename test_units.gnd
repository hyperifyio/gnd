# Test identity function
# Input: test-value
# Output: result
/gnd/identity result test-value
/gnd/format _ "Identity test: %s" test-value

# Test is_even function
# Input: 42
# Output: is-even-result
/gnd/is_even is-even-result 42
/gnd/format _ "Is even test: %t" true

# Test is_long function
# Input: "hello"
# Output: is-long-result
/gnd/is_long is-long-result "hello"
/gnd/format _ "Is long test: %t" true

# Test add function
# Input: 40 2
# Output: add-result
/gnd/add add-result 40 2
/gnd/format _ "Add test: %d" 42

# Test concat function
# Input: "hello" "world"
# Output: concat-result
/gnd/concat concat-result "hello" "world"
/gnd/format _ "Concat test: %s" "helloworld"

# Test inc function
# Input: 41
# Output: inc-result
/gnd/inc inc-result 41
/gnd/format _ "Inc test: %d" 42

# Test double function
# Input: 21
# Output: double-result
/gnd/double double-result 21
/gnd/format _ "Double test: %d" 42 