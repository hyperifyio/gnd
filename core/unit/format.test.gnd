# Test basic string formatting
/gnd/format _ _ "Hello, %s!" world
/gnd/string-eq _ Hello,world! _

# Test number formatting
/gnd/format _ _ "The answer is %d" 42
/gnd/string-eq _ The_answer_is_42 _

# Test multiple arguments
/gnd/format _ _ "%s %s %d" Hello world 42
/gnd/string-eq _ Hello_world_42 _

# Test special case: single percent sign
/gnd/format _ _ "%%"
/gnd/string-eq _ % _ 