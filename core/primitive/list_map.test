# Test list map primitive
# Empty list
prim-list-map [] "identity"

# Verify result
assert-equals len(result) 0

# List of integers
prim-list-map [1 2 3] "identity"

# Verify result
assert-equals result[0] 1
assert-equals result[1] 2
assert-equals result[2] 3

# List of strings
prim-list-map ["a" "b" "c"] "identity"

# Verify result
assert-equals result[0] "a"
assert-equals result[1] "b"
assert-equals result[2] "c"

# Invalid input type
prim-list-map "not a list" "identity"

# Verify error
assert-error "input is not a list"

# Invalid function token
prim-list-map [1 2 3] "nonexistent"

# Verify error
assert-error "invalid function token" 