# Test string match primitive
# Simple match
prim-string-match "Hello, World!" "World"

# Verify match
assert-equals matches[0].start 7
assert-equals matches[0].end 12
assert-equals matches[0].text "World"

# Multiple matches
prim-string-match "a1b2c3" "[0-9]"

# Verify matches
assert-equals matches[0].start 1
assert-equals matches[0].end 2
assert-equals matches[0].text "1"
assert-equals matches[1].start 3
assert-equals matches[1].end 4
assert-equals matches[1].text "2"
assert-equals matches[2].start 5
assert-equals matches[2].end 6
assert-equals matches[2].text "3"

# No matches
prim-string-match "Hello, World!" "xyz"

# Verify no matches
assert-equals len(matches) 0

# Empty string
prim-string-match "" ".*"

# Verify match
assert-equals matches[0].start 0
assert-equals matches[0].end 0
assert-equals matches[0].text "" 